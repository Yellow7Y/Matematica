package Matemática_no_java;

import java.util.Scanner;
import java.util.Vector;

import static java.lang.Math.*;

public class NúmerosDaMegaSena {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        Vector vector = new Vector();
        imprimirTraco();
        System.out.println("Quantos jogos você deseja realizar?");
        int numeroDeJogos = scanner.nextInt();
        int quantidade = numeroDeJogos;
        imprimirTraco();
        int contador = 0;
        while (contador == quantidade) {
            for (int i = 0;i < 6; i++) {
                int numeroAleatorio = (int) (random() * 60);
                System.out.println(numeroAleatorio);
        }
        }
    }
    private static void imprimirTraco() {
        System.out.println("=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-");
    }
    }
